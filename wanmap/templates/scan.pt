<html metal:use-macro="base">
  <tal:block metal:fill-slot="content">
    <div class="row">
      <h4>Parameters: ${scan.parameters}</h4>
      <tal:block tal:repeat="subscan scan.subscans" tal:condition="scan">
        <h5>${subscan.scanner_name}</h5>
        <p><tal:block repeat="target subscan.targets">${target.target} </tal:block></p>
        <pre id="${subscan.scanner_name}-results">${subscan.xml_results}</pre>
      </tal:block>
      <tal:block tal:condition="not scan"><p>No scans found.</p></tal:block>
    </div>
  </tal:block>
</html>
