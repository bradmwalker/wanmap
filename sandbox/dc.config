nat {
    destination {
        rule 1 {
            description "Port Forward: Redis to 10.1.0.10"
            destination {
                port 6379
            }
            inbound-interface eth0
            protocol tcp
            translation {
                address 10.1.0.10
            }
        }
    }
}
policy {
    route-map CONNECT {
        rule 10 {
            action permit
        }
    }
}
protocols {
    ospf {
        area 0 {
            network 10.0.0.0/8
            network 192.168.0.0/24
            network 203.0.113.0/24
        }
        default-information {
            originate {
                always
                metric 10
                metric-type 2
            }
        }
        log-adjacency-changes {
        }
        parameters {
            router-id 10.0.0.1
        }
        redistribute {
            connected {
                metric-type 2
                route-map CONNECT
            }
        }
    }
}
